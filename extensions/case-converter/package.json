{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "case-converter",
  "title": "Case Converter",
  "description": "Converts text into various text cases and copies the output to your clipboard.",
  "icon": "command-icon.png",
  "author": "phazonoverload",
  "categories": ["Data"],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Convert case",
      "description": "Template for submitting a form",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "to-case": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
